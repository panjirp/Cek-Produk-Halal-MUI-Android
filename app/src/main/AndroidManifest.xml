<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.panjirp.halalmui">

    <uses-permission android:name="android.permission.INTERNET"/>
    <application
        android:allowBackup="true"
        android:icon="@drawable/halal"
        android:label="@string/app_name"
        android:roundIcon="@drawable/halal"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />
                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".HasilCari"
            android:label="Semua"/>
        <activity android:name=".Kategori"
            android:label="Kategori"/>
        <activity android:name=".SelectedKategori"
            android:label="Selected"/>
    </application>

</manifest>